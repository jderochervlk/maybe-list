{
  "name": "rescript-project-template",
  "version": "0.0.1",
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "reason": { "react-jsx": 3 },
  "suffix": ".bs.js",
  "jsx": { "version": 4, "mode": "classic" },
  "bs-dependencies": [
    "@rescript/react",
    "@glennsl/rescript-jest",
    "bs-dom-testing-library"
  ],
  "warnings": {
    "error": "+101"
  }
}
